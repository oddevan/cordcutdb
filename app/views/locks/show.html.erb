<div class="page-header">
  <%= link_to locks_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_lock_path(@lock), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show lock</h1>
</div>

<dl class="dl-horizontal">
  <dt>Tier:</dt>
  <dd><%= @lock.tier.name %></dd>

  <dt>Region:</dt>
  <dd><%= @lock.region.name %></dd>

</dl>
